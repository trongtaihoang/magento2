<?xml version="1.0" encoding="UTF-8"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  layout="1column" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
	<body>
		<referenceContainer name="head.additional">
			<block class="Magento\Framework\View\Element\Template" name="store.map.script" template="MGS_StoreLocator::google_map_script.phtml"/>
		</referenceContainer>
		
		<referenceContainer name="columns.top">
			<block class="MGS\StoreLocator\Block\View" name="store.map.embed" template="MGS_StoreLocator::map.phtml" after="page.main.title" before="page.messages"/>
		</referenceContainer>
	</body>
</page>
